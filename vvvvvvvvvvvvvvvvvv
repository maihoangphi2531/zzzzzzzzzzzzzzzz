wait(10)
-- HÃ m Ä‘á»ƒ cháº¡y vÃ²ng láº·p gá»­i "UnequipItem"
local function unequipLoop()
    -- Äá»£i game táº£i hoÃ n toÃ n trÆ°á»›c khi báº¯t Ä‘áº§u vÃ²ng láº·p
    if not game:IsLoaded() then
        game.Loaded:Wait() -- Chá» game hoÃ n toÃ n táº£i
    end

    -- Láº¥y tÃªn ngÆ°á»i chÆ¡i hiá»‡n táº¡i
    local playerName = game.Players.LocalPlayer.Name

    while true do
        -- Kiá»ƒm tra PlayerEquips vÃ  Hat1 cÃ³ tá»“n táº¡i trÆ°á»›c khi cháº¡y
        local playerEquips = game:GetService("ReplicatedStorage"):WaitForChild("PlayerEquips", 10)
        if not playerEquips then
            warn("PlayerEquips khÃ´ng tá»“n táº¡i, thá»­ láº¡i sau...")
            wait(1) -- Chá» trÆ°á»›c khi láº·p láº¡i
            continue
        end

        local playerFolder = playerEquips:FindFirstChild(playerName)
        if not playerFolder then
            warn("KhÃ´ng tÃ¬m tháº¥y thÆ° má»¥c cho ngÆ°á»i chÆ¡i: " .. playerName)
            wait(1) -- Chá» trÆ°á»›c khi láº·p láº¡i
            continue
        end

        local hat1 = playerFolder:FindFirstChild("Hat1")
        if not hat1 then
            warn("Hat1 khÃ´ng tá»“n táº¡i cho ngÆ°á»i chÆ¡i: " .. playerName)
            wait(1) -- Chá» trÆ°á»›c khi láº·p láº¡i
            continue
        end

        -- XÃ¡c Ä‘á»‹nh cÃ¡c Ä‘á»‘i sá»‘
        local args = { [1] = hat1 }

        -- Gá»­i yÃªu cáº§u "UnequipItem"
        game:GetService("ReplicatedStorage"):WaitForChild("Shared"):WaitForChild("Inventory"):WaitForChild("UnequipItem"):FireServer(unpack(args))

        -- Chá» má»™t khoáº£ng thá»i gian trÆ°á»›c khi láº·p láº¡i
        wait(1) -- TÃ¹y chá»‰nh thá»i gian chá»
    end
end

-- Cháº¡y vÃ²ng láº·p ngay láº­p tá»©c trong má»™t coroutine
task.spawn(unequipLoop)

-- CÃ¡c logic khÃ¡c cá»§a báº¡n
if not game:IsLoaded() then
    game.Loaded:Wait() -- Chá» game hoÃ n toÃ n táº£i
end
local TeleportService = game:GetService("TeleportService")
local isTeleporting = false -- Biáº¿n khÃ³a Ä‘á»ƒ Ä‘áº£m báº£o cÃ¡c hoáº¡t Ä‘á»™ng khÃ¡c dá»«ng láº¡i khi teleportXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

